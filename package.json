  "version": "0.0.2",
  "description": "A Chat Bot for Live Coding Streamers",
  "main": "http://localhost:3000/index.html",
  "node-remote": "http://localhost",
  "node-main": "app.js",
  "chromium-args": "--data-path='./saved/'",
  "window": {
    "title": "Code Stream Guard",
    "width": 216,
    "height": 384,
    "background-color": "#000000",

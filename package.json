  "bugs": {
    "url": "https://github.com/LongSteve/code-stream-guard/issues"
  },
  "homepage": "https://github.com/LongSteve/code-stream-guard#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "express": "^4.14.0",
    "feedparser": "^1.1.5",
    "jsonfile": "^2.4.0",
    "lodash": "^4.16.4",
    "node-schedule": "^1.1.1",
    "node-xmpp-client": "^3.0.2",
    "nw": "^0.18.0",
    "request": "^2.75.0",
    "socket.io": "^1.5.0"
